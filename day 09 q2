#include<stdio.h>
int main()
{
	printf("Priyanshu Arora\nSAP ID: 590020080\nCourse: Bca \nBatch: 4");
	printf("\n-----------------\n");
	int n, i;
	printf("Enter the number whose table you want: "); scanf("%d", &n);
	for(i=1; i<=10; i++)
