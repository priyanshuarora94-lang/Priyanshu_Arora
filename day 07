	int a, b;
	printf("Enter two numbers:"); scanf("%d %d", &a, &b);
	if(a>b)
	{
		printf("%d is bigger!", a);
	}
	else if(b>a)
	{
		printf("%d is bigger!", b);
	}
	else
	{
		printf("Both are equal!");
	}
	return 0;
}
