/* 100 days code; Day 12: code2*/
#include<stdio.h>
int main()
{
	printf("Priyanshu Arora\nSAP ID: 590020080\nCourse: Bca \nBatch: 4");
	printf("\n-----------------\n");
	int a, b, c;
	printf("Enter the number and its power:"); scanf("%d %d", &a, &b);
	c=a;
	for(b; b>1; b--)
	{
		a=a*c;
	}
	printf("The result is: %d", a);
	return 0;
}
